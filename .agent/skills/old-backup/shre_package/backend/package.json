{
    "name": "shre-backend",
    "version": "1.0.0",
    "description": "Backend API for shre - Employee Data & Document Management Platform",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:migrate": "prisma migrate dev",
        "db:generate": "prisma generate",
        "db:seed": "ts-node prisma/seed.ts"
    },
    "keywords": [
        "employee-management",
        "document-management",
        "express",
        "typescript",
        "graphql"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.971.0",
        "@aws-sdk/s3-request-presigner": "^3.971.0",
        "@prisma/client": "^5.7.1",
        "aws-sdk": "^2.1520.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-graphql": "^0.12.0",
        "express-rate-limit": "^7.1.5",
        "graphql": "^16.8.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.9.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "redis": "^5.10.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.6",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.0",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "prisma": "^5.7.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
