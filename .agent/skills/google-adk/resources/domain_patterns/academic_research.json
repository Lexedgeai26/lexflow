{
    "domain": "Academic Research",
    "backend": {
        "agents": [
            {
                "name": "literature_reviewer",
                "instructions": "Synthesize findings from multiple academic papers on a specific topic.",
                "tools": [
                    "query_arxiv",
                    "summarize_paper"
                ]
            },
            {
                "name": "citation_manager",
                "instructions": "Ensure all claims are backed by properly formatted BibTeX or APA citations.",
                "tools": [
                    "validate_citations",
                    "generate_bibliography"
                ]
            }
        ]
    },
    "frontend": {
        "rich_schemas": {
            "ReferenceList": {
                "layout": "list",
                "sorting": "date_desc",
                "fields": [
                    "title",
                    "authors",
                    "journal",
                    "year"
                ]
            }
        }
    }
}